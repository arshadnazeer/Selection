<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arsh.carselection">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".presentation.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CarSelection">
        <activity
            android:name=".presentation.year.YearActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.model.ModelActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.manufacturer.ManufacturerActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>